            <Font name="Script MT Bold" size="36.0" />
         </font>
      </Text>
      <VBox layoutY="72.0" prefHeight="526.0" prefWidth="879.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <HBox prefHeight="41.0" prefWidth="1058.0" spacing="10.0">
               <children>
                  <Label text="Minimum Price:">
                     <HBox.margin>
                        <Insets left="8.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="min_price" prefHeight="22.0" prefWidth="60.0" promptText="0"/>
                  <Label text="Maximum Price:" />
                  <TextField fx:id="max_price" prefHeight="22.0" prefWidth="60.0" promptText="1000"/>
                  <ComboBox fx:id="sortColor" prefHeight="13.0" prefWidth="160.0" promptText="Main Color" style="-fx-background-color: #c6acef;">
