<AnchorPane fx:id="anchor_pane" prefHeight="642.0" prefWidth="879.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.client.CreateCustomMadeController">
   <children>
      <Text layoutX="211.0" layoutY="48.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Let's Create You'r Own Product...">
